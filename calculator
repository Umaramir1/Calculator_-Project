using System;

class Program
{
    static void Main()
    {
        // Declare two numbers
        double num1 = 10;
        double num2 = 5;

        // Perform multiplication
        double result = num1 * num2;

        // Output the result
        Console.WriteLine("The result of multiplying " + num1 + " and " + num2 + " is: " + result);
    }
}
