using System;

class Program
{
    static void Main()
    {
        // Declare two numbers
        double num1 = 10;
        double num2 = 2;

        // Perform division
        double result = num1 / num2;

        // Output the result
        Console.WriteLine("The result of dividing " + num1 + " by " + num2 + " is: " + result);
    }
}
