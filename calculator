using System;

class Program
{
    static void Main()
    {
        // Declare two numbers
        double num1 = 10;
        double num2 = 5;

        // Perform subtraction
        double result = num1 - num2;

        // Output the result
        Console.WriteLine("The result of subtracting " + num2 + " from " + num1 + " is: " + result);
    }
}
